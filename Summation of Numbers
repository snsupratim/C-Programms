// Sumation Of Numbers divisible by 5:
#include<stdio.h>
int main()
{
	int i,d,sum=0;
	for(i=100;i<=300;i++)
	{
		d=i%5;
		if(d==0)
		{
			sum=sum+i;
		}
	}
	printf("the sumation is % d",sum);
	return 0;
}
// Divisible by 7:
#include<stdio.h>
int main()
{
	int i,s=0;
	printf("number are divisible by 7 are....");
	for(i=100;i<=150;i++)
	{
		if(i%7==0)
		{
			printf("\n %8d is divisible",i);
			s+=i;
		}
		else
		{
			printf("\n %8d isnot divisible",i);
		}
	}
	printf("\n the sum id %d",s);
	return 0;
}
//Sumation of 1st 20 numbers:
 #include<stdio.h>
 int main()
 {
 	int i,num,sum;
 	for(i=1;i<=20;i++)
 	{
 		printf("1");
 		sum=1;
 		for(num=2;num<=i;num++)
 		{
 			printf("+%d",num);
 			sum+=num;
		 }
		 printf("=%d\n",sum);
	 }
	 return 0;
 }

// Sumation of prime numbers
#include <stdio.h>

int isPrime(int num) {
	int i;
    if (num <= 1)
        return 0;

    for ( i = 2; i * i <= num; i++) {
        if (num % i == 0)
            return 0;
    }

    return 1;
}

int main() {
    int n;
    int count = 0;
    int number = 2;
    int sum = 0;

    printf("Enter the value of n: ");
    scanf("%d", &n);

    while (count < n) {
        if (isPrime(number)) {
            sum += number;
            count++;
        }
        number++;
    }

    printf("Sum of the first %d prime numbers is: %d\n", n, sum);

    return 0;
}
